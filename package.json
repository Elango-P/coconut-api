{
  "name": "coconut-api",
  "version": "1.0.46",
  "description": "Coconut API",
  "engines": {
    "node": "16.17.0",
    "npm": "9.2.0"
  },
  "private": true,
  "main": "server.js",
  "scripts": {
    "prestart": "npm run sequelize db:migrate && nodemon",
    "test": "npm run lint -s",
    "sequelize": "./node_modules/.bin/sequelize $*",
    "lint": "eslint '**/*.js' --ignore-path .gitignore",
    "setup": "echo \"//registry.npmjs.org/:_authToken=${NPM_TOKEN}\" >> .npmrc",
    "dev": "npx supervisor server.js",
    "prod": "npm install && pm2 reload ecosystem.config.js --env production"
  },
  
  "dependencies": {
    "@aws-sdk/client-s3": "^3.662.0",
    "@slack/web-api": "^6.8.1",
    "archiver": "^5.3.1",
    "async": "^3.2.4",
    "aws-sdk": "^2.1286.0",
    "axios": "^1.2.2",
    "bcrypt": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "change-case": "^4.1.2",
    "date-fns": "^2.30.0",
    "db-migrate": "^0.11.13",
    "db-migrate-mysql": "^2.2.0",
    "debug": "^4.3.4",
    "dotenv": "^16.0.1",
    "ejs": "^3.1.8",
    "email-templates": "^2.7.1",
    "excel4node": "^1.8.0",
    "expo-server-sdk": "^3.10.0",
    "fast-csv": "^4.3.6",
    "google-auth-library": "^8.2.0",
    "jira-connector": "^3.1.0",
    "lodash": "^4.17.21",
    "loggly": "^1.1.1",
    "luxon": "^3.5.0",
    "md5": "^2.3.0",
    "mime-types": "^2.1.35",
    "moment": "^2.29.4",
    "moment-range": "^4.0.2",
    "mysql": "^2.18.1",
    "mysql2": "^2.3.3",
    "node-trello": "1.3.0",
    "nodemon": "^2.0.19",
    "pg": "8.9.0",
    "request": "2.88.2",
    "request-promise": "^4.2.6",
    "restify": "^7.7.0",
    "restify-cors-middleware": "^1.1.1",
    "restify-errors": "^8.0.2",
    "sendgrid": "^5.2.3",
    "sequelize": "^6.21.3",
    "sequelize-cli": "^6.4.1",
    "sharp": "^0.31.3",
    "slug": "^8.2.2",
    "string-strip-html": "^13.0.6",
    "tesseract.js": "^5.1.1",
    "textilejs": "1.0.2",
    "tmp": "0.2.1",
    "to-textile": "^0.1.1",
    "twilio": "^4.17.0",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "chai": "4.3.7",
    "eslint": "8.31.0",
    "husky": "^8.0.3",
    "mocha": "10.2.0",
    "prettier": "^2.8.3",
    "pretty-quick": "^3.1.3",
    "proxyquire": "2.1.3",
    "sinon": "^1.17.7",
    "sinon-as-promised": "^4.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
